@charset "utf-8";body,nav,dl,dt,dd,p,h1,h2,h3,h4,ul,ol,li,input,button,textarea,footer{margin:0;padding:0}
body{
  font:16px/1.5 "Microsoft YaHei","Lucida Sans Unicode","Myriad Pro","Hiragino Sans GB","Heiti SC",Verdana,simsun;color:#333;background:#fff;-webkit-text-size-adjust:none;min-width:320px
  }

h1,h2,h3,h4,h5,h6{
   font-size:100%}form{display:inline}
ul,ol{list-style:none}
a{text-decoration:none;color:#1a1a1a}
